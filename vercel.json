{
  "builds": [
    {
      "src": "foodie/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "foodie/wsgi.py"
    }
  ],
  "functions":/(.*) {
    "api": /(.*){
      "runtime": "python3.9"
    }
  }
}
